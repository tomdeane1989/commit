{
  "name": "sales-commission-api",
  "version": "1.2.6",
  "type": "module",
  "scripts": {
    "start": "npx prisma migrate deploy && npx prisma generate && node server-working.js",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "dev": "nodemon server-working.js",
    "migrate": "prisma migrate dev",
    "migrate:safe": "node migrate-safe.js",
    "migrate:reset": "echo 'Use npm run migrate:reset:force with ALLOW_DATABASE_RESET=true' && exit 1",
    "migrate:reset:force": "node migrate-safe.js reset",
    "generate": "prisma generate",
    "seed": "echo 'Use npm run seed:safe instead' && exit 1",
    "seed:safe": "node seed-data-safe.js",
    "seed:unsafe": "node seed-data.js",
    "backup": "node backup-database.js",
    "backup:before:migrate": "node backup-database.js && npm run migrate"
  },
  "dependencies": {
    "@hubspot/api-client": "^13.1.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "expr-eval": "^2.0.2",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "googleapis": "^153.0.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.7.0",
    "joi": "^17.11.0",
    "json-rules-engine": "^7.3.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "node-mailjet": "^6.0.9",
    "p-queue": "^8.1.0",
    "prisma": "^5.7.0",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.7.1",
    "validator": "^13.15.15",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
